{
  "name": "coda-test",
  "version": "1.0.0",
  "description": "A coding assessment for Coda Payments",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "compile": "rimraf dist/ && tsc -d -p . && copyfiles package.json .npmignore dist",
    "development": "DOTENV=.env ts-node-dev --respawn -- src/index.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint --fix src --ext .ts",
    "generate-html": "pip3 install pyyaml && python3 swagger-to-html.py < src/swagger.yaml > api.html",
    "test": "jest --passWithNoTests",
    "prepush": "npm run lint && npm run compile && npm run test"
  },
  "author": "Bo",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1111.0",
    "convict": "^6.2.1",
    "convict-format-with-validator": "^6.2.0",
    "csv-parse": "^5.0.4",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.0.0",
    "err-code": "^3.0.1",
    "express": "^4.17.2",
    "express-healthcheck": "^0.1.0",
    "fs": "0.0.1-security",
    "lodash": "^4.17.21",
    "mongoose": "^6.2.10",
    "multer": "^1.4.4",
    "openapi-validator-middleware": "^3.2.5",
    "path": "^0.12.7",
    "uuid": "^8.3.2",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/chance": "^1.1.3",
    "@types/convict": "^6.1.1",
    "@types/convict-format-with-validator": "^6.0.2",
    "@types/csvtojson": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.179",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.14",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "chance": "^1.1.8",
    "copyfiles": "^2.4.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^27.5.1",
    "jest-config": "^27.5.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  }
}
